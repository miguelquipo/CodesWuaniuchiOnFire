<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/HomeFrameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/graphicfondoinicio"
    tools:context=".HomeFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="396dp"
            android:layout_height="737dp"
            android:layout_marginTop="-200dp"
            android:layout_marginBottom="-60dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.65999997">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="match_parent"
                    android:layout_height="69dp" />

                <androidx.cardview.widget.CardView
                    android:id="@+id/cardComponetes"
                    android:layout_width="363dp"
                    android:layout_height="291dp"
                    android:layout_gravity="center"
                    app:cardCornerRadius="15dp"
                    app:cardElevation="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="368dp">

                        <ImageView
                            android:id="@+id/imageView3"
                            android:layout_width="368dp"
                            android:layout_height="199dp"
                            android:layout_gravity="center"
                            android:layout_marginBottom="316dp"
                            android:src="@drawable/componentes"
                            android:translationY="5dp"
                            app:cardCornerRadius="15dp"
                            app:cardElevation="30dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.523"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <TextView
                            android:id="@+id/textView5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="18dp"
                            android:fontFamily="sans-serif-black"
                            android:text="Componetes"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.113"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imageView3"
                            app:layout_constraintVertical_bias="0.0" />

                        <Button
                            android:id="@+id/mrInfoComponetes"
                            android:layout_width="88dp"
                            android:layout_height="36dp"
                            android:layout_marginTop="32dp"
                            android:backgroundTint="@color/ColorLogo"
                            android:onClick="navegarAActividad"
                            android:text="Ver más"
                            android:textSize="10sp"
                            app:cardElevation="30dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.942"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imageView3" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="match_parent"
                    android:layout_height="19dp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="363dp"
                    android:layout_height="291dp"
                    android:layout_gravity="center"
                    app:cardCornerRadius="15dp"
                    app:cardElevation="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="420dp">

                        <ImageView
                            android:id="@+id/imgEspecify"
                            android:layout_width="368dp"
                            android:layout_height="199dp"
                            android:layout_gravity="center"
                            android:layout_marginBottom="316dp"
                            android:src="@drawable/espesifications"
                            android:translationY="5dp"
                            app:cardCornerRadius="15dp"
                            app:cardElevation="30dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.523"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <TextView
                            android:id="@+id/componetesTv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="18dp"
                            android:fontFamily="sans-serif-black"
                            android:text="Especificaciones"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.113"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imgEspecify"
                            app:layout_constraintVertical_bias="0.0" />

                        <Button
                            android:id="@+id/mrinfoEspecificaciones"
                            android:layout_width="88dp"
                            android:layout_height="36dp"
                            android:layout_marginTop="32dp"
                            android:backgroundTint="@color/ColorLogo"
                            android:text="Ver más"
                            android:textSize="10sp"
                            app:cardElevation="30dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.941"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/imgEspecify" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />

                <androidx.cardview.widget.CardView
                    android:layout_width="363dp"
                    android:layout_height="291dp"
                    android:layout_gravity="center"
                    app:cardCornerRadius="15dp"
                    app:cardElevation="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent">

                        <ImageView
                            android:id="@+id/Armado"
                            android:layout_width="368dp"
                            android:layout_height="199dp"
                            android:layout_gravity="center"
                            android:layout_marginTop="8dp"
                            android:layout_marginBottom="92dp"
                            android:src="@drawable/imgconexion"
                            android:translationY="5dp"
                            app:cardCornerRadius="15dp"
                            app:cardElevation="30dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />

                        <TextView
                            android:id="@+id/armadotv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="18dp"
                            android:fontFamily="sans-serif-black"
                            android:text="Conexiónes"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.113"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/Armado"
                            app:layout_constraintVertical_bias="0.0" />

                        <Button
                            android:id="@+id/mrinfoConexiones"
                            android:layout_width="88dp"
                            android:layout_height="36dp"
                            android:layout_marginTop="32dp"
                            android:backgroundTint="@color/ColorLogo"
                            android:text="Ver más"
                            android:textSize="10sp"
                            app:cardElevation="30dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.941"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/Armado" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="match_parent"
                    android:layout_height="19dp" />

                <androidx.cardview.widget.CardView
                    android:layout_width="363dp"
                    android:layout_height="291dp"
                    android:layout_gravity="center"
                    app:cardCornerRadius="15dp"
                    app:cardElevation="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="368dp">

                        <ImageView
                            android:id="@+id/diagramas1"
                            android:layout_width="368dp"
                            android:layout_height="199dp"
                            android:layout_gravity="center"
                            android:layout_marginBottom="316dp"
                            android:src="@drawable/imggroup"
                            android:translationY="5dp"
                            app:cardCornerRadius="15dp"
                            app:cardElevation="30dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.523"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintVertical_bias="0.0" />

                        <TextView
                            android:id="@+id/equipotrabajo"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="18dp"
                            android:fontFamily="sans-serif-black"
                            android:text="Equipo"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.113"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/diagramas1"
                            app:layout_constraintVertical_bias="0.0" />

                        <Button
                            android:id="@+id/mrinfoEquipo"
                            android:layout_width="88dp"
                            android:layout_height="36dp"
                            android:layout_marginTop="32dp"
                            android:backgroundTint="@color/ColorLogo"
                            android:text="Ver más"
                            android:textSize="10sp"
                            app:cardElevation="30dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintHorizontal_bias="0.941"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/diagramas1" />
                    </androidx.constraintlayout.widget.ConstraintLayout>

                </androidx.cardview.widget.CardView>

            </LinearLayout>
        </ScrollView>

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="58dp"
            android:layout_height="60dp"
            android:layout_marginTop="15dp"
            android:src="@drawable/logo_splash"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>